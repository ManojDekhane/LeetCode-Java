class Solution {
    public int[] diStringMatch(String s) {
        int n = s.length();
        int[] arr = new int[n+1];
        int k = 0;
        int low = 0, high = n;

        for (char ch : s.toCharArray()) {
            if (ch == 'I') {
                arr[k++] = low++;
            } else if (ch == 'D') {
                arr[k++] = high--;
            }
        }

        arr[k] = low;

        return arr;
    }
}
