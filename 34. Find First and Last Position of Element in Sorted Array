class Solution {
    public int[] searchRange(int[] nums, int target) {
        int first = -1, second = -1;

        if (nums.length == 1 && nums[0] == target) {
            first = 0;
            second = 0;
        }

        for (int i = 0; i < nums.length; i++) {
            if (nums[i] == target && first == -1) {
                first = i;
            } else if (nums[i] == target && first != -1) {
                second = i;
            }
        }

        if (first != -1 && second == -1) {
            second = first;
        }

        return new int[]{first, second};
    }
}
